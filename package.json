{
  "name": "box-gen",
  "version": "0.0.0",
  "description": "Generates an GNU ARM Eclipse project from box.js files.",
  "main": "dist/index.js",
  "bin": {
    "box-gen": "bin/box-gen.js"
  },
  "scripts": {
    "compile": "node_modules/.bin/rimraf dist/* && node_modules/.bin/babel src -d dist",
    "prepublish": "npm run compile",
    "watch": "node_modules/.bin/babel --watch src -d dist",
    "test": "node_modules/.bin/mocha --require babel",
    "start": "node dist"
  },
  "author": "Vaughan Rouesnel",
  "license": "MIT",
  "dependencies": {
    "chalk": "^1.1.1",
    "cheerio": "~0.18.0",
    "glob": "^5.0.14",
    "interpret": "^0.6.6",
    "jsondiffpatch": "~0.1.31",
    "lodash": "^3.10.1",
    "mkdirp": "~0.5.0",
    "node-dir": "^0.1.10",
    "pretty-data": "^0.40.0",
    "prettydiff": "~1.11.1",
    "prettyjson": "~1.1.0",
    "rechoir": "^0.6.2",
    "underscore": "~1.8.2",
    "yargs": "^3.26.0"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-eslint": "^4.1.3",
    "eslint": "^1.5.1",
    "gulp": "^3.9.0",
    "gulp-eslint": "^1.0.0",
    "gulp-load-plugins": "^0.10.0",
    "mocha": "^2.3.3",
    "rimraf": "^2.4.3"
  }
}
